<p>
<table>
<tr>
  <td><%=l :user_last_name_form%></td>
  <td><%=l :user_first_name_form%></td>
  <td>Admin</td>
  <td>Bekreftet</td>
</tr>
<% for @user in @users %>
<tr>
  <td><%=h @user.last_name%></td>
  <td><%=h @user.first_name%></td>
  <td><%='*' if @user.admin?%></td>
  <td><%='*' if @user.verified?%></td>
  <td><%=link_to l(:user_edit_head), :action => :edit, :id => @user%></td>
</tr>
<% end %>
</table>
</p>

<br/>

<p>
<%=link_to 'Legg til ny bruker', :action => :signup%>
