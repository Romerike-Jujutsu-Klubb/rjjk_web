<% @content_width = 1024 %>

<pre>
<%= @raw_incoming_email.content.force_encoding('ASCII').
    encode('utf-8', :invalid => :replace, :undef => :replace, :replace => '?') %>
</pre>

<%= link_to 'Edit', edit_raw_incoming_email_path(@raw_incoming_email), class: 'btn btn-primary' %>
<%= link_to 'Back', raw_incoming_emails_path, class: 'btn btn-default' %>
