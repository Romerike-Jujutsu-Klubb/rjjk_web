<h1>Listing survey_requests</h1>

<table>
  <thead>
    <tr>
      <th>Survey</th>
      <th>Member</th>
      <th>Completed at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_requests.each do |survey_request| %>
      <tr>
        <td><%= link_to survey_request.survey.title, survey_request.survey %></td>
        <td><%= link_to survey_request.member.name, survey_request.member %></td>
        <td><%= survey_request.completed_at %></td>
        <td><%= link_to 'Show', survey_request %></td>
        <td><%= link_to 'Edit', edit_survey_request_path(survey_request) %></td>
        <td><%= link_to 'Destroy', survey_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey request', new_survey_request_path %>
