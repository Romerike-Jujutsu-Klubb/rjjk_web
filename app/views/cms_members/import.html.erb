<h1>Resultat fra import (<%=@new_members.size%> nye og <%=@updated_members.size%> endrede)</h1>

<% unless @new_members.empty? %>
<table class="members" border="0" cellpadding="5" cellspacing="1">
  <tr>
    <th><%= link_to 'Etternavn' %></th>
    <th><%= link_to 'Fornavn' %></th>
  </tr>
  
<% for member in @new_members %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td><%=h member.last_name %></td>
    <td><%=h member.first_name %></td>
    <td><%= link_to 'Rediger', :action => 'edit', :id => member %></td>
  </tr>
  <% for k, v in member.attributes %>
  <tr>
	<td><%=k%></td>
	<td align="left"><%=v%></td>
  </tr>
  <% end %>
<% end %>
</table>
<% end %>

<% unless @updated_members.empty? %>
<table class="members" border="0" cellpadding="5" cellspacing="1">
  <tr>
    <th><%= link_to 'Etternavn' %></th>
    <th><%= link_to 'Fornavn' %></th>
    <th colspan="2">Endringer</th>
  </tr>
  
<% for member, old_values in @updated_members %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td colspan="2"><%=link_to h("#{member.first_name} #{member.last_name}"), :action => :edit, :id => member.id %></td>
    <td align="right">Fra</td>
    <td align="center">-&gt;</td>
    <td align="left">Til</td>
  </tr>
  <% for k, v in old_values %>
  <tr>
  	<td/>
	<td>
		<%=k%>
	</td>
	<td align="right"><%=v%></td>
	<td align="center">-&gt;</td>
	<td align="left"><%=member[k]%></td>
  </tr>
  <% end %>
<% end %>
</table>
<% end %>
