table.table
  thead
    tr
      th Belte
      th Antall
  tbody
    - @graduation.graduates.select { |g| g.passed != false && g.member.current_rank(@graduation.group.martial_art_id, @graduation.held_on)&.colour != g.rank.colour }.sort_by(&:rank).group_by { |gr| gr.rank.colour }.each do |colour, graduates|
      tr
        th = colour
        td = graduates.size

- if @graduation.shopping_list_sent_at
  h4 Handleliste ble sendt #{@graduation.shopping_list_sent_at.to_date}.