<h1>Listing group_schedules</h1>

<table>
  <tr>
    <th>Group</th>
    <th>Weekday</th>
    <th>Start at</th>
    <th>End at</th>
  </tr>

<% for group_schedule in @group_schedules %>
  <tr>
    <td><%=h group_schedule.group_id %></td>
    <td><%=h group_schedule.weekday %></td>
    <td><%=h group_schedule.start_at %></td>
    <td><%=h group_schedule.end_at %></td>
    <td><%= link_to 'Show', group_schedule %></td>
    <td><%= link_to 'Edit', edit_group_schedule_path(group_schedule) %></td>
    <td><%= link_to 'Destroy', group_schedule, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New group_schedule', new_group_schedule_path %>
