<h1>Medlemmer med feil kontrakt</h1>
<table>
  <tr>
    <th>Navn</th>
    <th>Alder</th>
    <th>Kontrakt</th>
  </tr>
  <% @wrong_contracts.each do |nkfm| %>
      <tr>
        <td><%=link_to nkfm.member.name, :controller => :members, :action => :edit, :id => nkfm.member_id %></td>
        <td><%= nkfm.member.age %></td>
        <td><%= nkfm.kont_sats %></td>
      </tr>
  <% end %>
</table>
