<h2>Oppm√∏te for <%=@member.name%></h2>
<h3>
  Fra <%=@graduate ? "gradering (#{@graduate.graduation.held_on})" : "innmelding (#{@member.joined_on})"%>
  til <%= @date ? @date : 'i dag' %>
</h3>

<table border="1" width="100%"  >
  <tr>
    <th>Dato</th>
    <th>Gruppe</th>
  </tr>

<% for attendance in @attendances %>
  <tr>
    <td><%=attendance.date %></td>
    <td><%=attendance.group_schedule.group.name %></td>
  </tr>
<% end %>
  <tr>
    <th>Totalt:</th>
    <th><%= @attendances.size %></th>
  </tr>
</table>
