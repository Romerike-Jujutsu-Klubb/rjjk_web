<h1>Listing page_aliases</h1>

<table>
  <tr>
    <th>Old path</th>
    <th>New path</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @page_aliases.each do |page_alias| %>
  <tr>
    <td><%= page_alias.old_path %></td>
    <td><%= page_alias.new_path %></td>
    <td><%= link_to 'Show', page_alias %></td>
    <td><%= link_to 'Edit', edit_page_alias_path(page_alias) %></td>
    <td><%= link_to 'Destroy', page_alias, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

