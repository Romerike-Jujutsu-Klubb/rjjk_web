- chart_width = column_width_sm
- if true || !Rails.env.development?
  - history_chart_link = line_chart member_reports_history_graph_data_path, points: false, legend: false, width: '258px', height: '194px', title: 'Medlemmer'
  - attendance_chart_link = line_chart history_graph_data_attendances_path, points: false, legend: false, width: '258px', height: '194px', title: 'Oppmøte'
  - grades_chart_link = area_chart member_reports_grade_history_graph_data_path, points: false, legend: false, width: '258px', height: '194px', colors: %i[yellow orange green blue brown grey #444444 black].reverse, title: 'Beltegrader voksne'
  - age_chart_link = column_chart member_reports_age_chart_data_path, legend: false, width: '258px', height: '194px', title: 'Aldersfordeling'
  - good_attendance_chart_link = area_chart member_reports_grade_history_graph_data_path(params.slice(:interval, :step, :percentage)), points: false, legend: false, width: '258px', height: '194px', colors: %i[yellow orange green blue brown grey #444444 black].reverse, title: 'Jevnt oppmøte voksne'
- else
  - history_chart_link = 'Medlemshistorikk'
  - attendance_chart_link = 'Oppmøte'
  - grades_chart_link = 'Historikk over grader'
  - age_chart_link = 'Aldersfordeling'
  - good_attendance_chart_link = 'Jevnt oppmøte voksne'

.mb-3
  .mb-1 = link_to history_chart_link, member_reports_path
  .mb-1 = link_to attendance_chart_link, controller: :attendances, action: :report
  .mb-1 = link_to grades_chart_link, member_reports_grade_history_graph_path
  .mb-1 = link_to age_chart_link, member_reports_age_chart_path
  .mb-1 = link_to good_attendance_chart_link, member_reports_grade_history_graph_path(percentage: 33, step: 28, interval: 92)
