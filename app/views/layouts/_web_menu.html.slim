h1 data-toggle="collapse" data-target="#web-menu" role="button" aria-expanded="false" aria-controls="collapseExample"
  i.fa.fa-chevron-down.mx-3.position-absolute style='right:0'
  i.fa.fa-network-wired.fa-fw>
  | Web
ul#web-menu.collapse data-parent="#main-menu"
  li
    - if admin?
      a.btn.btn-.border-0.float-right href=new_news_item_path title='Ny nyhet' data-toggle=:tooltip
        i.fa.fa-plus
    = menu_item 'Nyheter', controller: :news
  li = menu_item 'Forsideseksjoner', front_page_sections_path
  li = menu_item 'Informasjonssider', information_pages_path
  li = menu_item 'Arrangement', events_path
  li = menu_item 'Brukere', controller: :users, action: :index
  li = link_to 'GitHub', 'https://github.com/Romerike-Jujutsu-Klubb/rjjk_web', target: 'github'
  li = menu_item 'Status', controller: :status
