<%= a.group_schedule.group.name %><br/>
<% [['Var der!', Attendance::Status::ATTENDED],
        ['Instruerte', Attendance::Status::INSTRUCTOR],
        ['Var syk', Attendance::Status::SICK],
        ['Bortreist', Attendance::Status::HOLIDAY],
        ['Forhindret', Attendance::Status::ABSENT],
   ].each do |text, status| %>
    [<%= link_to text, UserSystem.with_login(@member.user,
        controller: :attendances, action: :review, year: a.practice.year,
        week: a.practice.week, status: status,
        group_schedule_id: a.practice.group_schedule_id) %>]&nbsp;&nbsp;
<% end %>
