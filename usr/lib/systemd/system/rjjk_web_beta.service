[Unit]
Description=RJJK web application beta

[Service]
Type=forking
User=capistrano
Group=capistrano
WorkingDirectory=/u/apps/rjjk_web_beta/current
ExecStart=/u/apps/rjjk_web_beta/current/etc/init.d/rjjk_web_beta start
PIDFile=/u/apps/rjjk_web_beta/shared/pids/server.pid

# Timeout for start up/shut down
TimeoutSec=300

[Install]
WantedBy=graphical.target
